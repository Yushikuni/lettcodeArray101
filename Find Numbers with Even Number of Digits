public class Solution 
{
    public int FindNumbers(int[] nums) 
    {
        int countEven = 0;
        for (int i = 0; i < nums.Length; i++)
        {
            nums[i] = nums[i] / 10;
            //nums[i] = nums[i] % 10;
            if(nums[i] % 2 == 0)
            {
                ++countEven;
            }
        }
        return countEven;
    }
}
