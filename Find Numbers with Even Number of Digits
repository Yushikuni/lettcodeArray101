public class Solution 
{
    public int FindNumbers(int[] nums) 
    {
        int countEven = 0;
        int num = -1;
        for (int i = 0; i < nums.Length; i++)
        {
            num = nums[i] % 10;
            if(num % 2 == 0)
            {
                ++countEven;
            }
            num = nums[i] / 10;
        }
        return countEven;
    }
}
