public class Solution 
{
    public int RemoveElement(int[] nums, int val) 
    {  
        var index = -1;
        for (int i = 0; i < nums.Length; ++i)
        {
            if (nums[i] != val)
            {
                var temp = nums[i];
                nums[i] = nums[++index];
                nums[index] = temp;
            }
        }
        return index + 1;
    }
}
